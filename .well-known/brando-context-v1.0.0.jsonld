    "audioReferenceLink": { "@id": "brando:audioReferenceLink", "@type": "@id" },
    "audioUsageGuideline": { "@id": "brando:audioUsageGuideline", "@type": "xsd:string" },

    ## POSITIONING (1.4)
    "missionStatement": { "@id": "brando:missionStatement", "@type": "xsd:string" },
    "visionStatement": { "@id": "brando:visionStatement", "@type": "xsd:string" },
    "coreValues": { "@id": "brando:coreValues", "@container": "@set" },
    "brandPromise": { "@id": "brando:brandPromise", "@type": "xsd:string" },
    "audiencePersona": { "@id": "brando:audiencePersona", "@type": "@json" },
    "marketPosition": { "@id": "brando:marketPosition", "@type": "xsd:string" },
    "brandNarrative": { "@id": "brando:brandNarrative", "@type": "xsd:string" },
    "competitorContext": { "@id": "brando:competitorContext", "@type": "@json" },
    "domainContext": { "@id": "brando:domainContext", "@type": "@json" },

    ## GOVERNANCE (1.5)
    "guardRails": { "@id": "brando:guardRails", "@type": "@json" },
    "refusalStrategies": { "@id": "brando:refusalStrategies", "@type": "@json" },
    "complianceTags": { "@id": "brando:complianceTags", "@type": "@json" },
    "reviewWorkflow": { "@id": "brando:reviewWorkflow", "@type": "@json" },
    "updatePolicy": { "@id": "brando:updatePolicy", "@type": "@json" },
    "visibilityRating": { "@id": "brando:visibilityRating", "@type": "xsd:string" },
    "retrievableInLLM": { "@id": "brando:retrievableInLLM", "@type": "xsd:boolean" },
    "embeddingQuality": { "@id": "brando:embeddingQuality", "@type": "xsd:string" },
    "riskScenarios": { "@id": "brando:riskScenarios", "@type": "@json" },

    ## ASSETS (1.6)
    "logoAsset": { "@id": "brando:logoAsset", "@type": "@id" },
    "fontAsset": { "@id": "brando:fontAsset", "@type": "@id" },
    "imageLibrary": { "@id": "brando:imageLibrary", "@type": "@id" },
    "videoAsset": { "@id": "brando:videoAsset", "@type": "@id" },
    "designSystemLink": { "@id": "brando:designSystemLink", "@type": "@id" },
    "templateLibrary": { "@id": "brando:templateLibrary", "@type": "@id" },
    "guidelinesPDF": { "@id": "brando:guidelinesPDF", "@type": "@id" },
    "assetRepository": { "@id": "brando:assetRepository", "@type": "@id" },
    "personaAsset": { "@id": "brando:personaAsset", "@type": "@id" },
    "accessPolicy": { "@id": "brando:accessPolicy", "@type": "@id" },
    "qaAsset": { "@id": "brando:qaAsset", "@type": "@id" },
    "productAsset": { "@id": "brando:productAsset", "@type": "@id" },
    "serviceAsset": { "@id": "brando:serviceAsset", "@type": "@id" },
    "featuresAsset": { "@id": "brando:featuresAsset", "@type": "@id" },
    "datasetAsset": { "@id": "brando:datasetAsset", "@type": "@id" },
    "knowledgeAsset": { "@id": "brando:knowledgeAsset", "@type": "@id" }
  }
}
