{
  "@context": {
    "@version": 1.1,
    
    "brando": "https://brandoschema.com/vocab/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",

    "Brand": "brando:BrandObject",

    "name": {
      "@id": "https://schema.org/name",
      "@type": "xsd:string"
    },
    "description": {
      "@id": "https://schema.org/description",
      "@type": "xsd:string"
    },

    ## VERBAL IDENTITY (1.1)
    "toneOfVoice": { "@id": "brando:toneOfVoice", "@type": "xsd:string" },
    "dialogueStyle": { "@id": "brando:dialogueStyle", "@type": "xsd:string" },
    "writingStyle": { "@id": "brando:writingStyle", "@type": "xsd:string" },
    "namingConvention": { "@id": "brando:namingConvention", "@type": "xsd:string" },
    "brandPersona": { "@id": "brando:brandPersona", "@type": "@json" },
    "approvedTerms": { "@id": "brando:approvedTerms", "@container": "@set" },
    "prohibitedTerms": { "@id": "brando:prohibitedTerms", "@container": "@set" },
    "keyMessage": { "@id": "brando:keyMessage", "@type": "xsd:string" },
    "taglines": { "@id": "brando:taglines", "@container": "@set" },
    "promptScaffold": { "@id": "brando:promptScaffold", "@type": "@json" },
    "mustDo": { "@id": "brando:mustDo", "@type": "@json" },
    "mustNotDo": { "@id": "brando:mustNotDo", "@type": "@json" },

    ## VISUAL IDENTITY (1.2)
    "logo": { "@id": "brando:logo", "@type": "@id" },
    "colourPalette": { "@id": "brando:colourPalette", "@type": "@json" },
    "typography": { "@id": "brando:typography", "@type": "@json" },
    "iconography": { "@id": "brando:iconography", "@type": "@json" },
    "imageryStyle": { "@id": "brando:imageryStyle", "@type": "@json" },
    "motionRule": { "@id": "brando:motionRule", "@type": "@json" },
    "visualToken": { "@id": "brando:visualToken", "@type": "xsd:string" },
    "visualUsageGuideline": { "@id": "brando:visualUsageGuideline", "@type": "xsd:string" },
    "visualReferenceLink": { "@id": "brando:visualReferenceLink", "@type": "@id" },

    ## AUDIO IDENTITY (1.3)
    "audioLogo": { "@id": "brando:audioLogo", "@type": "@id" },
    "voiceSignature": { "@id": "brando:voiceSignature", "@type": "@json" },
    "pronunciationGuide": { "@id": "brando:pronunciationGuide", "@type": "@json" },
    "audioCue": { "@id": "brando:audioCue", "@type": "@json" },
